{
  "name": "ember-click-outside-modifier",
  "version": "4.1.0",
  "description": "Ember modifier to catch events outside marked DOM element",
  "keywords": [
    "ember-addon",
    "ember-modifier"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:lifeart/ember-click-outside-modifier.git"
  },
  "license": "MIT",
  "author": "Aleksandr Kanunnikov <lifeart92@gmail.com>",
  "files": [
    "addon-main.js",
    "dist",
    "types"
  ],
  "scripts": {
    "build": "rollup --config",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "rollup --config",
    "start": "rollup --config --watch"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.5.0",
    "ember-modifier": "^3.0.0 || ^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.8",
    "@embroider/addon-dev": "^1.5.0",
    "@rollup/plugin-babel": "^5.3.1",
    "@tsconfig/ember": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^10.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "rollup": "^2.70.2",
    "typescript": "^5.1.6"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "main": "addon-main.js",
    "type": "addon",
    "version": 2,
    "app-js": {
      "./modifiers/click-outside.js": "./dist/_app_/modifiers/click-outside.js"
    }
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./types/*.d.ts",
      "default": "./dist/*.js"
    },
    "./addon-main.js": "./addon-main.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "types/*"
      ]
    }
  }
}
